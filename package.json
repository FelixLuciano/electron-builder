{
  "name": "electron-builder",
  "description": "Electron configuration generator",
  "version": "1.0.0",
  "author": "Luciano Felix Dias <luciano.felix.555@hotmail.com>",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot --config ./webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config ./webpack.config.production.js"
  },
  "dependencies": {
    "vue": "^2.5.3",
    "vue-the-mask": "^0.11.1",
    "vuetify": "^1.3.0"
  },
